{
  "rewrites": [
    { "source": "/signup", "destination": "/signup.html" },
    { "source": "/auth-signin", "destination": "/auth-signin.html" },
    { "source": "/auth", "destination": "/auth.html" },
    { "source": "/survey", "destination": "/survey.html" },
    { "source": "/profile", "destination": "/profile.html" },
    { "source": "/partner-connect", "destination": "/partner-connect.html" },
    { "source": "/report-generator", "destination": "/report-generator.html" },
    { "source": "/test-auth", "destination": "/test-auth.html" },
    { "source": "/", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" }
      ]
    }
  ]
}
