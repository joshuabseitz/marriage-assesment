{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)\\.js$",
      "dest": "/$1.js",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    {
      "src": "/(.*)\\.css$",
      "dest": "/$1.css",
      "headers": {
        "Content-Type": "text/css; charset=utf-8"
      }
    },
    {
      "src": "/(.*)\\.json$",
      "dest": "/$1.json",
      "headers": {
        "Content-Type": "application/json; charset=utf-8"
      }
    },
    {
      "src": "/signup",
      "dest": "/signup.html"
    },
    {
      "src": "/auth-signin",
      "dest": "/auth-signin.html"
    },
    {
      "src": "/auth",
      "dest": "/auth.html"
    },
    {
      "src": "/survey",
      "dest": "/survey.html"
    },
    {
      "src": "/profile",
      "dest": "/profile.html"
    },
    {
      "src": "/partner-connect",
      "dest": "/partner-connect.html"
    },
    {
      "src": "/report-generator",
      "dest": "/report-generator.html"
    },
    {
      "src": "/test-auth",
      "dest": "/test-auth.html"
    },
    {
      "src": "/page([0-9]+)",
      "dest": "/page$1.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/lib/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

